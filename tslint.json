{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "object-literal-sort-keys": false,
        "quotemark": [true, "single"],
        "no-angle-bracket-type-assertion": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "node_modules",
            "./front/src/proto/*.ts"
        ]
    }
}
